<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerLoginOperator">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="198.0" layoutY="62.0" text="Login do Operador">
                    <font>
                        <Font size="30.0" />
                    </font>
                </Label>
                <TextField fx:id="textFieldLogin" layoutX="243.0" layoutY="148.0" prefHeight="25.0" prefWidth="215.0" promptText="Login" />
                <PasswordField fx:id="passwordFieldSenha" layoutX="243.0" layoutY="190.0" prefHeight="25.0" prefWidth="215.0" promptText="Senha" />
                <Label layoutX="180.0" layoutY="148.0" text="Login:">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <Label layoutX="180.0" layoutY="190.0" text="Senha:">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <Button layoutX="394.0" layoutY="266.0" mnemonicParsing="false" onAction="#doLogin" prefHeight="38.0" prefWidth="80.0" text="Entrar">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
                <Button layoutX="180.0" layoutY="266.0" mnemonicParsing="false" onAction="#backMainPage" prefHeight="38.0" prefWidth="80.0" text="Voltar">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</VBox>